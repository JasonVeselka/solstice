{
  "name": "solstice",
  "version": "0.0.1",
  "description": "Simple Date Time picker",
  "main": "dist/solstice.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonVeselka/solstice.git"
  },
  "author": {
    "name": "Jason Veselka",
    "url": "https://github.com/JasonVeselka",
    "email": "jason.veselka@live.com"
  },
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "lint": "eslint src",
    "ci": "babel-node test/*.test.js && zuul -- test/*.test.js",
    "coveralls": "npm run compile && babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/blue-tape ./lib/*.js --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postcoveralls": "rimraf coverage",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "zuul": "babel-node test/*.test.js && zuul --local 9966 -- test/*.test.js"
  },
  "keywords": [
    "date",
    "time",
    "calendar",
    "vanillaJS"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "babel-istanbul": "0.4.1",
    "chai": "^3.2.0",
    "eslint": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.0",
    "gulp-karma": "0.0.4",
    "gulp-uglify": "^1.5.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.4",
    "karma-chrome-launcher": "^0.1.5",
    "karma-coverage": "^0.5.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-html-reporter": "^0.2.6",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^0.1.9",
    "karma-sinon-chai": "^0.2.0",
    "karma-spec-reporter": "0.0.13",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.0",
    "sinon-chai": "^2.6.0",
    "webpack": "^1.12.0"
  },
  "dependencies": {}
}
