{
  "name": "solstice",
  "version": "0.0.1",
  "description": "Simple Date Time picker",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonVeselka/solstice.git"
  },
  "author": {
    "name": "Jason Veselka",
    "url": "https://github.com/JasonVeselka"
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf reports",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "build": "./node_modules/.bin/babel --presets es2015,stage-0 -d dist/ src/",
    "coveralls": "npm run cover -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf coverage",
    "prebuild": "npm run lint -s && npm run clean -s",
    "postinstall": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "prepublish": "npm run build -s",
    "postpublish": "git push origin master --follow-tags",
    "watch": "node_modules/.bin/babel src -d lib --experimental -w"
  },
  "keywords": [
    "date",
    "time",
    "calendar",
    "vanillaJS"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-istanbul": "0.4.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.2.0",
    "coveralls": "2.11.4",
    "eslint": "^1.3.1",
    "eslint-config-xo": "^0.9.1",
    "eslint-plugin-babel": "2.1.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.4",
    "karma-chrome-launcher": "^0.1.5",
    "karma-coverage": "^0.5.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-html-reporter": "^0.2.6",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^0.1.9",
    "karma-sinon-chai": "^0.2.0",
    "karma-spec-reporter": "0.0.13",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.0",
    "rimraf": "^2.5.0",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14"
  }
}
