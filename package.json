{
  "name": "solstice",
  "version": "0.0.1",
  "description": "Simple Date Time picker",
  "main": "dist/solstice.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonVeselka/solstice.git"
  },
  "author": {
    "name": "Jason Veselka",
    "url": "https://github.com/JasonVeselka",
    "email": "jason.veselka@live.com"
  },
  "scripts": {
    "watch": "node_modules/.bin/babel src -d lib --experimental -w",
    "clean": "rimraf reports",
    "lint": "eslint src",
    "ci": "babel-node test/*.test.js && zuul -- test/*.test.js",
    "build": "npm run clean && babel --stage 0 -d dist src",
    "test": "babel-node node_modules/.bin/blue-tape test/*.test.js | tspec",
    "coveralls": "npm run build && babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/blue-tape ./lib/*.js --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postcoveralls": "rimraf coverage",
    "postinstall": "npm run build",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "zuul": "babel-node test/*.test.js && zuul --local 9966 -- test/*.test.js"
  },
  "keywords": [
    "date",
    "time",
    "calendar",
    "vanillaJS"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.33",
    "babel-eslint": "4.1.3",
    "babel-istanbul": "0.4.1",
    "babelify": "6.3.0",
    "blue-tape": "0.1.10",
    "chai": "^3.2.0",
    "coveralls": "2.11.4",
    "eslint": "^1.3.1",
    "eslint-config-xo": "^0.9.1",
    "eslint-plugin-babel": "2.1.1",
    "rimraf": "^2.5.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "zuul": "^3.8.0"
  },
  "dependencies": {}
}
