{
  "name": "solstice",
  "version": "0.0.8",
  "description": "Simple Date Time picker",
  "main": "dist/solstice.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonVeselka/solstice.git"
  },
  "author": {
    "name": "Jason Veselka",
    "url": "https://github.com/JasonVeselka",
    "email": "jason.veselka@live.com"
  },
  "scripts": {
    "prebuild": "npm run lint -s && npm run clean -s",
    "build": "npm run build-js && npm run build-css && npm run min",
    "build-js": "babel --presets es2015 src/ -d dist/",
    "watch-js": "babel src -d dist -w",
    "build-css": "cat src/*.css | cleancss -o dist/solstice.css",
    "watch-css": "catw src/*.css -o dist/solstice.css -v",
    "watch": "npm run watch-js & npm run watch-css",
    "lint": "eslint src",
    "clean": "rimraf reports",
    "test": "NODE_ENV=development karma start",
    "test-ci": "NODE_ENV=test karma start",
    "coveralls": "coveralls < resport-lcov/**/*lcov.info",
    "postcoveralls": "rimraf coverage",
    "prepublish": "npm run build",
    "postpublish": "git push origin master --follow-tags",
    "min": "uglifyjs dist/solstice.js -o dist/solstice.min.js"
  },
  "keywords": [
    "date",
    "time",
    "picker",
    "calendar",
    "vanillaJS",
    "babel6",
    "es6"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-core": "6.3.x",
    "babel-eslint": "5.0.0-beta6",
    "babel-istanbul": "0.6.x",
    "babel-istanbul-loader": "0.0.1",
    "babel-loader": "6.2.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "catw": "1.0.1",
    "chai": "3.2.0",
    "clean-css": "3.4.9",
    "coveralls": "2.11.4",
    "eslint": "1.10.3",
    "eslint-config-xo": "0.9.1",
    "eslint-plugin-babel": "3.0.0",
    "karma": "0.13.9",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-coveralls": "1.1.2",
    "karma-env-preprocessor": "0.1.1",
    "karma-firefox-launcher": "0.1.7",
    "karma-mocha": "0.1.9",
    "karma-sinon-chai": "0.2.0",
    "karma-sourcemap-loader": "0.3.6",
    "karma-spec-reporter": "0.0.13",
    "karma-webpack": "1.7.0",
    "loader-utils": "0.2.12",
    "mocha": "2.3.4",
    "rimraf": "2.5.0",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "uglify": "^0.1.5",
    "webpack": "1.12.0"
  },
  "dependencies": {}
}
